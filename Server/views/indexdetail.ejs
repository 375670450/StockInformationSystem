<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | Top Navigation</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="https://cdn.bootcss.com/admin-lte/2.3.11/css/AdminLTE.min.css">

    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="https://cdn.bootcss.com/admin-lte/2.3.11/css/skins/_all-skins.min.css">

    <link rel="stylesheet" href="/application/indexdetail.css">
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-table/1.11.1/bootstrap-table.min.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<!-- ADD THE CLASS layout-top-nav TO REMOVE THE SIDEBAR. -->
<body class="hold-transition skin-blue layout-top-nav" onload="onload()">


<% include layout %>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper" >
    <!-- Main content -->
    <section class="content">
        <div class="row ">
            <div class="col-md-2 col-xs-6" style="padding-right: 0px; padding-left: 0px">
                <div class="transparent box box-solid bg-gray-light">
                    <div class="box-body">

                        <div class="panel box box-primary">
                            <div class="box-header with-border">
                                最近浏览
                            </div>
                            <div class="box-body text-muted text-sm no-padding">
                                <table class="table table-condensed no-header" id="stock-basic-table">
                                </table>
                            </div>
                        </div>

                        <div class="panel box box-primary">
                            <div class="box-header with-border">
                                操盘必读
                            </div>
                            <div class="box-body text-muted text-sm" id="company-info-list">

                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div class="col-md-10 col-xs-12">
                <div class="row">
                    <div class="col-md-12 col-xs-12">
                        <!-- small box -->
                        <div class="small-box bg-green">
                            <div class="inner">
                                <h3><span id="name">万科A</span> (<span id="code"><%= code %></span>)</h3>

                                <table class="table-responsive" id="realtime-table">
                                    <tr>
                                        <!--<td>-->
                                            <!--均价：<span class="badge bg-yellow-active" id="trade">&#45;&#45;</span>-->
                                        <!--</td>-->
                                        <!--<td>-->
                                            <!--现手：<span class="badge bg-yellow-active" id="curvolume">--</span>-->
                                        <!--</td>-->
                                        <!--<td>-->
                                            <!--换手率：<span class="badge bg-yellow-active" id="turnoverratio">&#45;&#45;</span>-->
                                        <!--</td>-->
                                        <!--<td>-->
                                            <!--量比：<span class="badge bg-yellow-active" id="high">--</span>-->
                                        <!--</td>-->
                                        <td>
                                            总手：<span class="badge bg-yellow-active" id="volume">--</span>
                                        </td>
                                        <td >
                                            最高：<span class="badge bg-yellow-active" id="high">--</span>
                                        </td>
                                        <td>
                                            涨停：<span class="badge bg-yellow-active" id="uplimit">--</span>
                                        </td>
                                        <td>
                                            昨收：<span class="badge bg-yellow-active" id="preclose">--</span>
                                        </td>
                                        <!---->
                                    </tr>
                                    <tr>
                                        <!--<td>-->
                                            <!--市盈：<span class="badge bg-yellow-active" id="turnoverratio">--</span>-->
                                        <!--</td>-->
                                        <td>
                                            总金额：<span class="badge bg-yellow-active" id="amount">--</span>
                                        </td>
                                        <td>
                                            最低：<span class="badge bg-yellow-active" id="low">--</span>
                                        </td>
                                        <td>
                                            跌停：<span class="badge bg-yellow-active" id="downlimit">--</span>
                                        </td>
                                        <td>
                                            今开：<span class="badge bg-yellow-active" id="open">--</span>
                                        </td>
                                        <td>
                                            振幅：<span class="badge bg-yellow-active" id="change">--</span>
                                        </td>
                                    </tr>
                                </table>

                                <p></p>
                            </div>
                            <div class="icon">
                                <i class="ion ion-stats-bars"></i>
                            </div>
                            <!--<a href="#" class="small-box-footer">-->
                            <!--More info <i class="fa fa-arrow-circle-right"></i>-->
                            <!--</a>-->
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6 col-xs-12">
                        <div class="box box-solid">
                            <ul class="nav nav-tabs bg-gray-light" id="curve-tab">
                                <li><a href="#">分时</a> </li>
                                <li class="active"><a href="#">日K</a> </li>
                                <li><a href="#">月K</a> </li>
                                <li><a href="#">5分钟K</a> </li>
                                <li><a href="#">15分钟K</a> </li>
                                <li><a href="#">30分钟K</a> </li>
                                <li><a href="#">60分钟K</a> </li>
                            </ul>
                            <div class="tab-content">
                                <div id="curve-day">

                                </div>
                            </div>

                        </div>
                    </div>

                    <div class="col-md-4 col-xs-6">
                        <div class="box box-info">
                            <div class="box-header with-border bg-gray-light">
                                <h3 class="box-title">明细</h3>
                                <div class="box-tools pull-right">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body">
                                <table id="trade-records-table" class="table table-hover table-bordered" data-striped="true">
                                </table>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-2 col-xs-6">
                        <div class="box box-info">
                            <div class="box-header with-border bg-gray-light">
                                <h3 class="box-title">报价</h3>
                                <div class="box-tools pull-right">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body">
                                <table id="price-table" class="table table-hover table-bordered" data-show-header="false" data-striped="true">
                                </table>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="row">
                    <div class="col-md-6 col-xs-12">
                        <div class="box box-solid">
                            <ul class="nav nav-tabs bg-gray-light" id="news-tab" >
                                <li><a data-toggle="tab" href="#stock-news-table-tab">个股新闻</a> </li>
                                <li class="active"><a data-toggle="tab" href="#financial-news-table-tab">相关新闻</a> </li>
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane fade" id="stock-news-table-tab">
                                    <table  id="stock-news-table" data-show-header="false"></table>
                                </div>
                                <div class="tab-pane fade in active" id="financial-news-table-tab">
                                    <table id="financial-news-table" data-show-header="false"></table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-xs-12">
                        <div class="box box-info">
                            <div class="box-header with-border bg-gray-light">
                                <h3 class="box-title">大单交易</h3>
                                <div class="box-tools pull-right">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body">
                                <table id="block-records-table" class="table table-hover table-bordered" data-striped="true">
                                </table>
                            </div>
                        </div>

                    </div>
                </div>

                <div class="row">

                    <div class="col-md-6 col-xs-12">
                        <div class="box box-solid">
                            <ul class="nav nav-tabs bg-gray-light" id="rank-tab">
                                <li><a data-toggle="tab" href="#industry-rank-table-tab">同行业个股</a> </li>
                                <li class="active"><a data-toggle="tab" href="#area-rank-table-tab">同地区个股</a> </li>
                                <span class="label label-default pull-right"><h5 id="rank-field">  </h5></span>
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane fade" id="industry-rank-table-tab">
                                    <table  id="industry-rank-table"></table>
                                </div>
                                <div class="tab-pane fade in active" id="area-rank-table-tab">
                                    <table id="area-rank-table"></table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-xs-12">
                        <div class="box box-info">
                            <div class="box-header with-border bg-gray-light">
                                <h3 class="box-title">净利润成长性</h3>
                                <div class="box-body">
                                    <div id="profit-chart"></div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->

<footer class="main-footer">
    <div class="pull-right hidden-xs">
        <b>Version</b> 1.0.0
    </div>

<!--Highstock-->
<script src="https://cdn.hcharts.cn/highstock/highstock.js"></script>

<script src="https://cdn.plot.ly/plotly-latest.min.js"></script>

<script src="/application/indexdetail.js"></script>

</body>
</html>
